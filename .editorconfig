root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
tab_width = 4
indent_style = space

[*.rs]
indent_size = 4

[*.{json,toml,yaml,md}]
indent_size= 2

[*.md]
max_line_length = off
insert_final_newline = false
trim_trailing_whitespace = false
